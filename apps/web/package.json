{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "electron:compile": "tsc -p tsconfig.electron.json",
    "electron:dev": "tsc -p tsconfig.electron.json && BUILD_TARGET=electron NODE_ENV=development electron .",
    "electron:build": "tsc -p tsconfig.electron.json && BUILD_TARGET=electron NODE_ENV=production next build --turbopack && electron-builder",
    "electron:build:mac": "tsc -p tsconfig.electron.json && BUILD_TARGET=electron NODE_ENV=production next build --turbopack && electron-builder --mac",
    "electron:build:win": "tsc -p tsconfig.electron.json && BUILD_TARGET=electron NODE_ENV=production next build --turbopack && electron-builder --win",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "check-types": "next typegen && tsc --noEmit",
    "prettier-check": "prettier --check .",
    "prettier": "prettier --write .",
    "code-check": "yarn prettier-check; yarn check-types ; yarn lint",
    "clean": "rm -rf .next",
    "ncu": "ncu --cooldown 2 -x eslint,@eslint/js",
    "ncuu": "ncu -u --cooldown 2 -x eslint,@eslint/js",
    "ncum": "ncu --target minor --cooldown 2",
    "ncumu": "ncu --target minor -u --cooldown 2"
  },
